<script type="text/javascript">
    $(".szt_ad3_tab_panel span").hover(function () {
        var data = $(this).attr("data");
        $(".szt_ad3_tab_panel span").removeClass("hover");
        $(this).addClass("hover");
        $(".szt_ad3_tab_panel_con div").hide();
        $("#szt_ad3_tab_panel_con_" + data).show();
    })
</script>

<script type="text/javascript">
    function zt_submit(id, type) {
        var name = $("#" + type + "_UserName").val();
        var tel = $("#" + type + "_UserTel").val();
        var house = $("#" + type + "_House").val();
        var code = $("#" + type + "_vCode").val();
        if ($.trim(name) == "") {
            layer.alert("请输入您的称呼!");
            return false;
        }
        if ($.trim(tel) == "") {
            layer.alert("请输入您的电话!");
            return false;
        }
        if ($.trim(house) == "") {
            layer.alert("请输入您的楼盘名称!");
            return false;
        }
        if ($.trim(code) == "") {
            layer.alert("请输入您的验证码!");
            return false;
        }
        $("#" + id).ajaxSubmit({
            url: "/home/fjoin",
            type: "post",
            dataType: "json",
            beforeSubmit: function () {
                layer.load(1, { shade: [0.1, '#000'] });
            },
            error: function () {
                layer.closeAll('loading');
                layer.alert("请求失败");
            },
            success: function (data) {
                layer.closeAll('loading');
                if (data != null) {
                    if (data.Success) {
                        layer.alert(data.Msg, function () {
                            window.location.href = window.location.href;
                        });
                    }
                    else
                        layer.alert(data.Msg);
                }
                else
                    layer.alert("提交失败");
            }
        });
        return false;
    }
</script>

