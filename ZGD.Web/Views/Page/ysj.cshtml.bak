@{
    ViewBag.Title = "第一届家装书画艺术节";
    ViewBag.Keywords = "港宏,宜家装饰,重庆装修公司,重庆装饰公司,重庆家装公司,整体家装,装修报价,环保家装";
    ViewBag.Description = "重庆港宏宜家装饰公司是港宏装饰集团旗下品牌，是一个集整体配送、整体设计、整体售后、整体环保、整体软装、一站式购齐的整体家装企业，重庆港宏宜家装饰公司在重庆装修公司、重庆装饰公司、重庆家装公司行、重庆环保装修等同行中有丰富的家装经验优势。重庆港宏宜家装饰公司承诺:质量不合格：“砸”、材料不环保：“赔”、服务不满意：“罚”。网络预约尽享实惠咨询热线:023-68526912。";
}

@section Head
{
    <style type="text/css">
        .b1 { height: 600px; background: url(/Content/Images/zt/ysj/01.jpg) center no-repeat; }
        .b2 { height: 197px; background: url(/Content/Images/zt/ysj/02.jpg) center no-repeat; }
        .b3 { height: 989px; background: url(/Content/Images/zt/ysj/03.jpg) center no-repeat; }
        .b4 { height: 1188px; background: url(/Content/Images/zt/ysj/04.jpg) center no-repeat; }
        .b5 { height: 1541px; background: url(/Content/Images/zt/ysj/05.jpg) center no-repeat; }
        .b5 .main a { width: 210px; height: 60px; display: inline-block; margin-top: 955px; margin-left: 490px; }
        .b6 { height: 1657px; background: url(/Content/Images/zt/ysj/06.jpg) center no-repeat; }
        .b7 { height: 652px; background: url(/Content/Images/zt/ysj/07.jpg) center no-repeat; }
        .b7 .main { padding-top: 110px; }
        .b7 .main .txt { width: 400px; height: 95px; line-height: 95px; font-size: 33px; font-weight: bold; color: #64615A; border: 0; font-family: "宋体"; background-color: transparent; padding-left: 35px; }
        .b7 .main .btn { width: 215px; height: 70px; background-color: transparent; display: inline-block; margin-left: 190px; margin-top: 36px; border: 0; cursor: pointer; }
        .b7 .main .form { padding-left: 60px; }
        .b8 { height: 266px; background: url(/Content/Images/zt/ysj/08.jpg) center no-repeat; }
    </style>
}
@{Html.RenderAction("Top", "Ctrl", new { menuID = 3 });}
<!--content begin-->
<div id="content">
    <div class="b1"></div>
    <div class="b2"></div>
    <div class="b3"></div>
    <div class="b4"></div>
    <div class="b5">
        <div class="main"><a href="#bm"></a></div>
    </div>
    <div class="b6"></div>
    <a name="bm"></a>
    <div class="b7">
        <div class="main">
            <form method="post" id="form_join" class="form">
                <table>
                    <tr>
                        <td><input type="text" class="txt" name="UserName" id="join_UserName" placeholder="姓名" /></td>
                        <td style="padding-left:200px;"><input type="text" class="txt" name="Phone" id="join_Phone" placeholder="电话" /></td>
                    </tr>
                    <tr>
                        <td style="padding-top: 22px;"><input type="text" class="txt" name="House" id="join_House" placeholder="楼盘" /></td>
                        @*<td><label>验证码：</label><input type="text" class="txt" name="vCode" id="join_vCode" style="width:70px;" /> <img src="/Tools/verify_code.ashx" onclick="ChangeCode('nl_join_code');" id="nl_join_code" style="height:35px;margin-left:2px;" /></td>*@
                        <td style="padding-top: 22px;">
                            <input type="submit" class="btn" value="  " id="btnSubmit" />
                            <input type="hidden" value="3" name="aID" />
                        </td>
                    </tr>
                </table>
            </form>
        </div>

        <script type="text/javascript">
            $('#form_join').submit(function () {
                var name = $("#join_UserName").val();
                var tel = $("#join_Phone").val();
                var house = $("#join_House").val();
                if ($.trim(name) == "") {
                    layer.alert("请输入您的称呼!");
                    return false;
                }
                if ($.trim(tel) == "") {
                    layer.alert("请输入您的电话!");
                    return false;
                }
                if ($.trim(house) == "") {
                    layer.alert("请输入您的楼盘名称!");
                    return false;
                }
                $(this).ajaxSubmit({
                    url: "/home/ajoinw",
                    type: "post",
                    dataType: "json",
                    beforeSubmit: function () {
                        layer.load(1, { shade: [0.1, '#000'] });
                    },
                    error: function () {
                        layer.closeAll('loading');
                        layer.alert("请求失败");
                    },
                    success: function (data) {
                        layer.closeAll('loading');
                        if (data != null) {
                            if (data.Success) {
                                layer.alert(data.Msg, function () {
                                    window.location.href = window.location.href;
                                });
                            }
                            else
                                layer.alert(data.Msg);
                        }
                        else
                            layer.alert("提交失败");
                    }
                });
                return false;
            });
        </script>
    </div>
    <div class="b8"></div>
</div>
<!--content end-->

