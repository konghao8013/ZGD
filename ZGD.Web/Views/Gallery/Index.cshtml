@{
    ViewBag.Title = ViewBag.Type;
    ViewBag.Keywords = "中国致公党重庆市委员会";
    ViewBag.Description = "中国致公党重庆市委员会";
}

@using ZGD.Common
@using System.Data
@{Html.RenderAction("Top", "Ctrl", new { menuID = ViewBag.TypeID });}

<div class="con_bg main">
    <div class="index_top" style="height:auto;">
        <div class="title"><span>您的位置：首页 &gt; 致公艺苑 @*&gt; @(ViewBag.Type)*@</span></div>
        <div class="index_top_con">
            <ul class="pic_page">
                @foreach (DataRow item in Model.Rows)
                {
                    <li><a href="/gallery/@(item["Id"].ToString()).html" title="@(item["Title"].ToString())"><img src="@(item["ImgUrl"].ToString())" alt="@(item["Title"].ToString())" /></a><p>@(item["Title"].ToString())</p></li>
                }
            </ul>
            <div class="pager">
                @Html.Raw(ViewBag.Pager)
            </div>
        </div>
    </div>
    <div class="space30"></div>
    <hr class="page_hr" />
</div>